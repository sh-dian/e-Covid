<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".View.Login.User_MenuPage">

    <RelativeLayout
        android:layout_width="391dp"
        android:layout_height="781dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="2dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="152dp"
            android:layout_height="45dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="115dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="124dp"
            android:text="Hi, I'm e-Covid"
            android:textColor="#FFFFFF"
            android:textSize="22sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <ImageButton
            android:layout_width="48dp"
            android:layout_height="62dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="11dp"
            android:layout_marginEnd="345dp"
            android:background="@null"
            android:contentDescription="@string/back_Button"
            android:src="@drawable/ic_baseline_arrow_back_24"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

        <ImageView
            android:layout_width="217dp"
            android:layout_height="211dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="71dp"
            android:layout_marginTop="109dp"
            android:layout_marginEnd="103dp"
            android:layout_marginBottom="461dp"
            android:contentDescription="@string/back_Button"
            android:src="@drawable/logo"
            tools:ignore="ImageContrastCheck" />

        <Button
            android:id="@+id/userProfile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="75dp"
            android:layout_marginTop="324dp"
            android:layout_marginEnd="94dp"
            android:layout_marginBottom="400dp"
            android:background="@drawable/button_corner"
            android:text="USER PROFILE"
            android:textColor="@color/white"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="75dp"
            android:layout_marginTop="390dp"
            android:layout_marginEnd="94dp"
            android:layout_marginBottom="334dp"
            android:background="@drawable/button_corner"
            android:text="CHECK-IN CHECK-OUT"
            android:textColor="@color/white"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/vaccination"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="75dp"
            android:layout_marginTop="458dp"
            android:layout_marginEnd="94dp"
            android:layout_marginBottom="266dp"
            android:background="@drawable/button_corner"
            android:text="VACCINATION"
            android:textColor="@color/white"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="75dp"
            android:layout_marginTop="528dp"
            android:layout_marginEnd="94dp"
            android:layout_marginBottom="196dp"
            android:background="@drawable/button_corner"
            android:text="QUARANTINE"
            android:textColor="@color/white"
            tools:ignore="HardcodedText,DuplicateSpeakableTextCheck" />

        <Button
            android:id="@+id/qcBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="75dp"
            android:layout_marginTop="597dp"
            android:layout_marginEnd="94dp"
            android:layout_marginBottom="127dp"
            android:background="@drawable/button_corner"
            android:text="QUARANTINE CENTER"
            android:textColor="@color/white"
            tools:ignore="HardcodedText,DuplicateSpeakableTextCheck" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>