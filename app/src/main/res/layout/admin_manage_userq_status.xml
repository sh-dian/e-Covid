                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView71"
                    android:layout_width="59dp"
                    android:layout_height="match_parent"
                    android:text="Name: "
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView114"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
