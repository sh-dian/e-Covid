<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.Login.MOH.MOH_MenuPage">

    <RelativeLayout
        android:layout_width="391dp"
        android:layout_height="781dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="2dp">

        <ImageView
            android:layout_width="217dp"
            android:layout_height="211dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="79dp"
            android:layout_marginTop="135dp"
            android:layout_marginEnd="95dp"
            android:layout_marginBottom="435dp"
            android:contentDescription="@string/back_Button"
            android:src="@drawable/logo"
            tools:ignore="ImageContrastCheck" />

        <Button
            android:id="@+id/mohVaccinationBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="79dp"
            android:layout_marginTop="423dp"
            android:layout_marginEnd="90dp"
            android:layout_marginBottom="301dp"
            android:background="@drawable/button_corner"
            android:text="MANAGE VACCINATION"
            android:textColor="@color/white"
            tools:ignore="HardcodedText,DuplicateSpeakableTextCheck" />

        <Button
            android:id="@+id/mohUserQuarantine_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="79dp"
            android:layout_marginTop="493dp"
            android:layout_marginEnd="90dp"
            android:layout_marginBottom="231dp"
            android:background="@drawable/button_corner"
            android:text="MANAGE USER QUARANTINE"
            android:textColor="@color/white"
            tools:ignore="HardcodedText,DuplicateSpeakableTextCheck" />

        <Button
            android:id="@+id/mohManageUserBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="81dp"
            android:layout_marginTop="355dp"
            android:layout_marginEnd="88dp"
            android:layout_marginBottom="369dp"
            android:background="@drawable/button_corner"
            android:text="MANAGE USER"
            android:textColor="@color/white"
            tools:ignore="HardcodedText,DuplicateSpeakableTextCheck" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>