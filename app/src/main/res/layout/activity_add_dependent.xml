<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.Login.addDependent">

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/background"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageButton
            android:layout_width="71dp"
            android:layout_height="88dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="-1dp"
            android:layout_marginEnd="341dp"
            android:background="@null"
            android:contentDescription="@string/back_Button"
            android:src="@drawable/ic_baseline_arrow_back_24" />

        <TextView
            android:id="@+id/textView47"
            android:layout_width="381dp"
            android:layout_height="36dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="105dp"
            android:layout_marginEnd="7dp"
            android:layout_marginBottom="590dp"
            android:background="@null"
            android:ems="10"
            android:text="Enter your dependents details"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="381dp"
            android:layout_height="36dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="130dp"
            android:layout_marginEnd="6dp"
            android:layout_marginBottom="565dp"
            android:background="@null"
            android:ems="10"
            android:text="All files are mandatory"
            android:textColor="@color/black"
            android:textSize="20sp" />

        <Button
            android:id="@+id/Add_dependents"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="139dp"
            android:layout_marginBottom="127dp"
            android:background="@drawable/radius"
            android:text="@string/add_dependents"
            android:textColor="#FFFFFF" />

        <android.support.design.widget.TextInputEditText
            android:id="@+id/name_dependent"
            android:layout_width="292dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="67dp"
            android:layout_marginTop="222dp"
            android:layout_marginEnd="51dp"
            android:layout_marginBottom="469dp"
            android:background="@drawable/radius"
            android:hint="Full Name"
            android:inputType="text"
            android:textColor="@color/white"
            android:textColorHint="#FFFFFF"
            tools:ignore="HardcodedText" />

        <android.support.design.widget.TextInputEditText
            android:id="@+id/passportNum"
            android:layout_width="292dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="67dp"
            android:layout_marginTop="279dp"
            android:layout_marginEnd="51dp"
            android:layout_marginBottom="412dp"
            android:background="@drawable/radius"
            android:hint="NRIC or Passport"
            android:inputType="number"
            android:textColor="@color/white"
            android:textColorHint="#FFFFFF"
            tools:ignore="HardcodedText" />

        <android.support.design.widget.TextInputEditText
            android:id="@+id/relation"
            android:layout_width="292dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="68dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="356dp"
            android:background="@drawable/radius"
            android:hint="Relation"
            android:inputType="number"
            android:textColor="@color/white"
            android:textColorHint="#FFFFFF"
            tools:ignore="HardcodedText" />

        <android.support.design.widget.TextInputEditText
            android:id="@+id/age"
            android:layout_width="292dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="68dp"
            android:layout_marginTop="335dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="356dp"
            android:background="@drawable/radius"
            android:hint="Age"
            android:inputType="number"
            android:textColor="@color/white"
            android:textColorHint="#FFFFFF"
            tools:ignore="HardcodedText" />

        <android.support.design.widget.TextInputEditText
            android:id="@+id/gender"
            android:layout_width="292dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="67dp"
            android:layout_marginTop="394dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="297dp"
            android:background="@drawable/radius"
            android:hint="Gender"
            android:inputType="number"
            android:textColor="@color/white"
            android:textColorHint="#FFFFFF"
            tools:ignore="HardcodedText" />

        <android.support.design.widget.TextInputEditText
            android:id="@+id/address"
            android:layout_width="292dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="68dp"
            android:layout_marginTop="450dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="241dp"
            android:background="@drawable/radius"
            android:hint="Current address"
            android:inputType="number"
            android:textColor="@color/white"
            android:textColorHint="#FFFFFF"
            tools:ignore="HardcodedText" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView46"
        android:layout_width="264dp"
        android:layout_height="53dp"
        android:background="@null"
        android:ems="10"
        android:text="@string/manage_dependent"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036" />

</android.support.constraint.ConstraintLayout>